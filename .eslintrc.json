{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "linebreak-style": "off",
    "arrow-body-style": "off",
    "implicit-arrow-linebreak": "off",
    "max-len": ["error", {
      "code": 155,
      "ignoreComments": true,
      "ignoreRegExpLiterals": true
    }],
    "react/prop-types": "off",
    "react/destructuring-assignment": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}